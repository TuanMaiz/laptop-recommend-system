@prefix : <http://example.org/laptop#> .
@prefix default1: <http://www.semanticweb.org/ontologies/laptop#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Battery a owl:Class ;
    rdfs:subClassOf :Specification .

:Brand a owl:Class ;
    rdfs:subClassOf :Specification .

:CPU a owl:Class ;
    rdfs:subClassOf :Specification .

:Gaming a owl:Class ;
    rdfs:subClassOf :Functionality .

:Graphic a owl:Class ;
    rdfs:subClassOf :Functionality .

:HighPrice a owl:Class ;
    rdfs:subClassOf :PriceRange .

:LowPrice a owl:Class ;
    rdfs:subClassOf :PriceRange .

:MidPrice a owl:Class ;
    rdfs:subClassOf :PriceRange .

:Office a owl:Class ;
    rdfs:subClassOf :Functionality .

:RAM a owl:Class ;
    rdfs:subClassOf :Specification .

:RefreshRate a owl:Class ;
    rdfs:subClassOf :Specification .

:ScreenSize a owl:Class ;
    rdfs:subClassOf :Specification .

:Storage a owl:Class ;
    rdfs:subClassOf :Specification .

:Weight a owl:Class ;
    rdfs:subClassOf :Specification .

:hasPrice a owl:DatatypeProperty ;
    rdfs:domain :Product ;
    rdfs:range xsd:decimal .

:hasPriceRange a owl:ObjectProperty ;
    rdfs:domain :Product ;
    rdfs:range :PriceRange .

:hasSpecification a owl:ObjectProperty ;
    rdfs:domain :Product ;
    rdfs:range :Specification .

:satisfiesRequirement a owl:ObjectProperty ;
    rdfs:domain :Product ;
    rdfs:range :Functionality .

default1:user_a-b-b-c a default1:User ;
    default1:HAS_FUNCREQ default1:Graphic ;
    default1:PREFERS_RANGE default1:HighEnd .

:Functionality a owl:Class .

:PriceRange a owl:Class .

:Product a owl:Class .

:Specification a owl:Class .

