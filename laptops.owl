@prefix : <http://example.org/laptop#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

############################
# Classes
############################

:Functionality rdf:type owl:Class .
:Specification rdf:type owl:Class .
:PriceRange rdf:type owl:Class .
:Product rdf:type owl:Class .

# Subclasses of Functionality
:Gaming rdf:type owl:Class ;
        rdfs:subClassOf :Functionality .

:Office rdf:type owl:Class ;
        rdfs:subClassOf :Functionality .

:Graphic rdf:type owl:Class ;
         rdfs:subClassOf :Functionality .

# Subclasses of Specification
:CPU rdf:type owl:Class ;
     rdfs:subClassOf :Specification .

:RAM rdf:type owl:Class ;
     rdfs:subClassOf :Specification .

:ScreenSize rdf:type owl:Class ;
            rdfs:subClassOf :Specification .

:RefreshRate rdf:type owl:Class ;
             rdfs:subClassOf :Specification .

:Storage rdf:type owl:Class ;
         rdfs:subClassOf :Specification .

:Weight rdf:type owl:Class ;
        rdfs:subClassOf :Specification .

:Battery rdf:type owl:Class ;
         rdfs:subClassOf :Specification .

:Brand rdf:type owl:Class ;
       rdfs:subClassOf :Specification .

# Subclasses of PriceRange
:LowPrice rdf:type owl:Class ;
          rdfs:subClassOf :PriceRange .

:MidPrice rdf:type owl:Class ;
          rdfs:subClassOf :PriceRange .

:HighPrice rdf:type owl:Class ;
           rdfs:subClassOf :PriceRange .


############################
# Object Properties
############################

:hasSpecification rdf:type owl:ObjectProperty ;
                  rdfs:domain :Product ;
                  rdfs:range :Specification .

:satisfiesRequirement rdf:type owl:ObjectProperty ;
                      rdfs:domain :Product ;
                      rdfs:range :Functionality .

:hasPriceRange rdf:type owl:ObjectProperty ;
               rdfs:domain :Product ;
               rdfs:range :PriceRange .

############################
# Data Properties
############################

:hasPrice rdf:type owl:DatatypeProperty ;
          rdfs:domain :Product ;
          rdfs:range xsd:decimal .

